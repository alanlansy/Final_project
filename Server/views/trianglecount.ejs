    <script type="text/javascript">

      var viz;

     
      function draw(q) {
          var config = {
              container_id: "viz",
              server_url: "bolt://graphdb.ga:7687",
              server_user: "neo4j",
              server_password: "password",
              labels: {
                "Person": {
                        "caption": "name",
                        "font-size":"1"
                    },
                    "Crime": {
                        "caption": "type",
                        "font-size":"1"
                    }
                  
              },
              relationships: {
                "KNOWS": {
                        "thickness": "2",
                        "caption": true
                    },
                    "PARTY_TO": {
                        "thickness": "2",
                        "caption": true
                    }
                  
              },
              initial_cypher: q
          };

          viz = new NeoVis.default(config);
          viz.render();
      }
     
  </script> 
  <script>
    var url = new URL(window.location.href);
    var c = url.searchParams.get("q");
    var query="MATCH (p1:Person {nhs_no: '"+c+"'})-[k1:KNOWS]-(p2)-[k2:KNOWS]-(p3)-[k3:KNOWS]-(p1) WITH * MATCH (person)-[pt:PARTY_TO]->(crime) WHERE person IN[p1, p2, p3] RETURN *";
    $("#q").val(query)
    draw($("#q").val())
    $("#sidebar").attr('class', 'shrinked');
